Cell[BoxData[{
 RowBox[{"clear", "[", 
  RowBox[{"string1", ",", "string2"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"anagramQ", "[", 
    RowBox[{"string1_", ",", "string2_"}], "]"}], " ", ":=", " ", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"Str1", ",", " ", "Str2"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Str1", "=", " ", 
       RowBox[{"StringReplace", "[", 
        RowBox[{"string1", ",", " ", 
         RowBox[{"\"\< \>\"", " ", "\[Rule]", " ", "\"\<\>\""}]}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"Str2", " ", "=", 
       RowBox[{"StringReplace", "[", 
        RowBox[{"string2", ",", " ", 
         RowBox[{"\"\< \>\"", " ", "\[Rule]", " ", "\"\<\>\""}]}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"Length", "[", "Str1", "]"}], "\[Equal]", 
          RowBox[{"Length", "[", "Str2", "]"}]}], " ", "&&", " ", 
         RowBox[{
          RowBox[{"Sort", "[", 
           RowBox[{"Characters", "[", 
            RowBox[{"ToLowerCase", "[", "Str1", "]"}], "]"}], "]"}], 
          "\[Equal]", 
          RowBox[{"Sort", "[", 
           RowBox[{"Characters", "[", 
            RowBox[{"ToLowerCase", "[", "Str2", "]"}], "]"}], "]"}]}]}], ",", 
        "  ", "\"\<Strings are Anagrams of each other\>\"", ",", " ", 
        "\"\<Strings are not Anagrams of each other\>\""}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]}], "Input",
