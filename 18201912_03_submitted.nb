Cell[BoxData[{
 RowBox[{"clear", "[", 
  RowBox[{"p", ",", "s1", ",", "s2"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"palindromeStringQ", "[", "p_", "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"StringQ", "[", "p", "]"}], ",", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"p", "\[Equal]", 
        RowBox[{"StringReverse", "[", "p", "]"}]}], ",", 
       "\"\<String is a Palindrome\>\"", ",", " ", 
       "\"\<String is not a Palindrome\>\""}], "]"}], ",", 
     "\"\<Input is not a string\>\""}], "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"anagramStringQ", "[", 
   RowBox[{"s1_", ",", "s2_"}], "]"}], " ", ":=", " ", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"StringQ", "[", "s1", "]"}], " ", "&&", " ", 
     RowBox[{"StringQ", "[", "s2", "]"}]}], ",", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Sort", "[", 
        RowBox[{"Characters", "[", 
         RowBox[{"ToLowerCase", "[", "s1", "]"}], "]"}], "]"}], "\[Equal]", 
       RowBox[{"Sort", "[", 
        RowBox[{"Characters", "[", 
         RowBox[{"ToLowerCase", "[", "s2", "]"}], "]"}], "]"}]}], ",", 
      "    ", "\"\<Strings are Anagrams of each other\>\"", ",", " ", 
      "\"\<Strings are not Anagrams of each other\>\""}], "]"}], ",", " ", 
    "\"\<Input is not a string\>\""}], "]"}]}]}], "Input",
